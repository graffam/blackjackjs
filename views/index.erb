<script>
  function Card (value, suit){
    this.cardValue = value;
    this.cardSuit = suit;
  }
  function Deck () {
    var deck = [];
    var suits = ["♠", "♥", "♦", "♣"];
    for (i = 0; i < suits.length; i++) {
      for (num = 1; num <= 10; num++) {
        deck.push(new Card(num, suits[i]))
      }
    }
    return deck
  }
  var deck = new Deck
  console.log(deck)
</script>
